package model;

import com.fasterxml.jackson.databind.*;

public class Fish {
    private String id;
    private String name;
    private String rarity;
    private double size;
    private double price;  
    public Fish(String id, String name, String rarity, double size, double price) {
        this.id = id;
        this.name = name;
        this.rarity = rarity;
        this.size = size;
        this.price = price;  
    }

    // Getter methods
    public String getId() {
        return id;
    }

    public String getName() {
        return name;
    }

    public String getRarity() {
        return rarity;
    }

    public double getSize() {
        return size;
    }

    public double getPrice() {
        return price;  // Return the price of the fish
    }

    public String jsonify() {
        
    }
}
