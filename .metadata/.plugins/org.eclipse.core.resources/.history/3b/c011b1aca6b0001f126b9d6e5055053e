package model;
public class Rod {
    private String id;
    private String name;
    private String rarity;
    private double price;

    public Rod(String id, String name, String rarity, double price) {
        this.id = id;
        this.name = name;
        this.rarity = rarity;
        this.price = price;
    }

    public String getId() {
        return id;
    }

    public String getName() {
        return name;
    }

    public String getRarity() {
        return rarity;
    }

    public double getPrice() {
        return price;
    }
}
